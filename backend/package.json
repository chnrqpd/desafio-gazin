{
  "name": "developers-api",
  "version": "1.0.0",
  "description": "API para Gerenciamento de Desenvolvedores",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "db:migrate": "npx sequelize-cli db:migrate",
